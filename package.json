{
  "name": "bookshelf",
  "version": "1.0.0",
  "description": "An app that uses the google books API to store and sort a user book selection",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "express": "^4.17.1",
    "mongoose": "^5.9.2"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BestBroBradley/final-homework.git"
  },
  "author": "Allison Bradley",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BestBroBradley/final-homework/issues"
  },
  "homepage": "https://github.com/BestBroBradley/final-homework#readme"
}
